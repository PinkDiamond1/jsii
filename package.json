{
  "private": true,
  "license": "Apache-2.0",
  "version": "1.57.0",
  "scripts": {
    "build": "turbo run build",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "bump": "bash scripts/bump.sh",
    "dist-clean": "turbo run dist-clean && rm -rf dist",
    "package": "bash scripts/package.sh",
    "test": "turbo run test --concurrency=1 && yarn compliance",
    "test:integ": "turbo run test:integ --concurrency=1",
    "test:update": "turbo run test:update --concurrency=1 && yarn compliance",
    "compliance": "(cd tools/jsii-compliance && yarn report)"
  },
  "devDependencies": {
    "@jest/types": "^27.5.1",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "all-contributors-cli": "^6.20.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest-circus": "^27.5.1",
    "jest-config": "^27.5.1",
    "prettier": "^2.6.2",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "turbo": "^1.2.6",
    "typescript": "~3.9.10",
    "wsrun": "^5.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aws/jsii.git"
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "packages/@jsii/*",
      "packages/@scope/*",
      "tools/*"
    ],
    "nohoist": [
      "**/@fixtures/jsii-calc-bundled",
      "**/@fixtures/jsii-calc-bundled/**"
    ]
  },
  "packageManager": "yarn@1.22.18"
}
