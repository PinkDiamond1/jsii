{
  "private": true,
  "license": "Apache-2.0",
  "version": "1.57.0",
  "scripts": {
    "build": "yarn nx run-many --target=build --all",
    "lint": "yarn nx run-many --target=lint --all",
    "lint:fix": "yarn nx run-many --target=lint:fix --all",
    "bump": "bash scripts/bump.sh",
    "dist-clean": "yarn nx run-many --target=dist-clean --all && rm -rf dist",
    "package": "bash scripts/package.sh",
    "test": "yarn nx run-many --target=test --all && yarn compliance",
    "test:integ": "yarn nx run-many --target=test:integ --all",
    "test:update": "yarn nx run-many --target=test:update --all",
    "compliance": "(cd tools/jsii-compliance && yarn report)"
  },
  "devDependencies": {
    "@jest/types": "^27.5.1",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "all-contributors-cli": "^6.20.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest-circus": "^27.5.1",
    "jest-config": "^27.5.1",
    "nx": "14.1.1",
    "prettier": "^2.6.2",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "~3.9.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aws/jsii.git"
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "packages/@jsii/*",
      "packages/@scope/*",
      "tools/*"
    ],
    "nohoist": [
      "**/@fixtures/jsii-calc-bundled",
      "**/@fixtures/jsii-calc-bundled/**"
    ]
  }
}
